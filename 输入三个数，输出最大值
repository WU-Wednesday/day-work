#include<stdio.h>
int main()
{
  int a,b,c;
  int max;
  printf("请输入三个数：");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b?a:b)
    {
      max=a;
      if(c>max?c:max)
      {
        printf("最大值是：%d\n",c);
      }
      else
      {
        printf("最大值是：%d\n",max);
      }
    }
    else
    {
      max=b;
      if(c>max?c:max)
      {
        printf("最大值是：%d\n",c);
      }
      else
      {
        printf("最大值是：%d\n",max);
      }
    }
    return 0;
}
