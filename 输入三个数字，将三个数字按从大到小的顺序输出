#include<stdio.h>
int main()
{
  int a,b,c;
  printf("请输入三个数字：");
  scanf("%d %d %d",&a,&b,&c);
  if(a>b&&b>c)
    {
      printf("从大到小排列：%d %d %d",a,b,c);
      }
  if(a>b&&b<c)
  {
      if(a>c)
      {
        printf("从大到小排列：%d %d %d",a,c,b);
      }
    else
    {
      printf("从大到小排列：%d %d %d",c,a,b);
    }
  }
  if(a<b&&b<c)
  {
    printf("从大到小排列：%d %d %d",c,b,a);
  }
  if(a<b&&b>c)
  {
      if(a>c)
      {
        printf("从大到小排列：%d %d %d",b,a,c);
      }
    else
    {
      printf("从大到小排列：%d %d %d",b,c,a);
    }
 }
  return 0;
}
